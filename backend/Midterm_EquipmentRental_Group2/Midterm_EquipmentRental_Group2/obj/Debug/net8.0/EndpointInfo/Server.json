  "components": {
    "schemas": {
      "Category": {
        "enum": [
          "HeavyMachinery",
          "PowerTools",
          "Vehicles",
          "Safety",
          "Surveying"
        ],
        "type": "string"
      },
      "Condition": {
        "enum": [
          "New",
          "Excellent",
          "Good",
          "Fair",
          "Poor"
        ],
        "type": "string"
      },
      "CreateCustomerDto": {
        "required": [
          "email",
          "name",
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "maxLength": 100,
            "minLength": 6,
            "type": "string",
            "format": "password"
          }
        },
        "additionalProperties": false
      },
      "CreateEquipmentDto": {
        "required": [
          "category",
          "description",
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "condition": {
            "$ref": "#/components/schemas/Condition"
          },
          "rentalPrice": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "Equipment": {
        "required": [
          "category",
          "description",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          },
          "category": {
            "$ref": "#/components/schemas/Category"
          },
          "condition": {
            "$ref": "#/components/schemas/Condition"
          },
          "rentalPrice": {
            "type": "number",
            "format": "double"
          },
          "isAvailable": {
            "type": "boolean"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "ExtendRentalRequest": {
        "type": "object",
        "properties": {
          "newDueDate": {
            "type": "string",
            "format": "date-time"
          },
          "reason": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IssueRentalRequest": {
        "required": [
          "customerId",
          "dueDate",
          "equipmentId"
        ],
        "type": "object",
        "properties": {
          "equipmentId": {
            "type": "integer",
            "format": "int32"
          },
          "customerId": {
            "type": "integer",
            "format": "int32"
          },
          "dueDate": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ReturnRentalRequest": {
        "type": "object",
        "properties": {
          "rentalId": {
            "type": "integer",
            "format": "int32"
          },
          "conditionOnReturn": {
            "type": "string",
            "nullable": true
          },
          "notes": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "UpdateCustomerDto": {
        "required": [
          "name",
          "username"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "username": {
            "minLength": 1,
            "type": "string"
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "role": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
