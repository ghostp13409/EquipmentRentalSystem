          },
          "isAvailable": {
            "type": "boolean"
          },
          "imageUrl": {
            "type": "string",
            "nullable": true
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "ExtendRentalRequest": {
        "type": "object",
        "properties": {
          "newDueDate": {
            "type": "string",
            "format": "date-time"
          },
          "reason": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Rental": {
        "required": [
          "customer",
          "dueDate",
          "equipment",
          "issuedAt"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "equipmentId": {
            "type": "integer",
            "format": "int32"
          },
          "equipment": {
            "$ref": "#/components/schemas/Equipment"
          },
          "customerId": {
            "type": "integer",
            "format": "int32"
          },
          "customer": {
            "$ref": "#/components/schemas/Customer"
          },
          "issuedAt": {
            "type": "string",
            "format": "date-time"
          },
          "dueDate": {
            "type": "string",
            "format": "date-time"
          },
          "returnedAt": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "conditionOnReturn": {
            "$ref": "#/components/schemas/Condition"
          },
          "notes": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "$ref": "#/components/schemas/Status"
          }
        },
        "additionalProperties": false
      },
      "ReturnRentalRequest": {
        "type": "object",
        "properties": {
          "rentalId": {
            "type": "integer",
            "format": "int32"
          },
          "conditionOnReturn": {
            "$ref": "#/components/schemas/Condition"
          },
          "notes": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
